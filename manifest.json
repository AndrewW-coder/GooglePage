{
  "name": "SearchDesk",
  "description": "A customizable google search screen with animated backgrounds.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_icon": "base_icon.png"
  },

  "chrome_url_overrides": {
    "newtab": "betHome.html"
  },

  "icons": {
    "128" : "base_icon.png"
  },

  "permissions": ["search"]
}